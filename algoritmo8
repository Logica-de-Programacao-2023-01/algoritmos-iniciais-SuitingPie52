leia lista
leia pesos
var somatorio_elementos = 0
var somatorio_pesos = 0
para cada elemento da lista faça:
	somatorio_elementos = somatorio_elementos + (elemento * peso_correspondente)
para cada peso dos pesos faça:
	somatorio_pesos = somatorio_pesos + peso
resultado = somatorio_elementoa / somatorio_pesos
escreva resultado
